	$(CC) $(CFLAGS_WII) $(INC_WII) -c LMP3D/NEW_CONSOLES/PL_Sample.c -o $(OBJDIR_WII)/LMP3D/NEW_CONSOLES/PL_Sample.o

clean_wii: 
	rm -f $(OBJ_WII) $(OUT_WII)
	rm -rf bin
	rm -rf $(OBJDIR_WII)
	rm -rf $(OBJDIR_WII)/LMP3D/All
	rm -rf $(OBJDIR_WII)/LMP3D/All/Load
	rm -rf $(OBJDIR_WII)/LMP3D/All/Graphics
	rm -rf $(OBJDIR_WII)/LMP3D/NEW_CONSOLES

before_testnewconsole: 
	test -d bin || mkdir -p bin
	test -d $(OBJDIR_TESTNEWCONSOLE) || mkdir -p $(OBJDIR_TESTNEWCONSOLE)
	test -d $(OBJDIR_TESTNEWCONSOLE)/LMP3D/All || mkdir -p $(OBJDIR_TESTNEWCONSOLE)/LMP3D/All
	test -d $(OBJDIR_TESTNEWCONSOLE)/LMP3D/All/Load || mkdir -p $(OBJDIR_TESTNEWCONSOLE)/LMP3D/All/Load
	test -d $(OBJDIR_TESTNEWCONSOLE)/LMP3D/All/Graphics || mkdir -p $(OBJDIR_TESTNEWCONSOLE)/LMP3D/All/Graphics
	test -d $(OBJDIR_TESTNEWCONSOLE)/LMP3D/NEW_CONSOLES || mkdir -p $(OBJDIR_TESTNEWCONSOLE)/LMP3D/NEW_CONSOLES

after_testnewconsole: 

testnewconsole: before_testnewconsole out_testnewconsole after_testnewconsole

out_testnewconsole: before_testnewconsole $(OBJ_TESTNEWCONSOLE) $(DEP_TESTNEWCONSOLE)
	$(LD) $(LDFLAGS_TESTNEWCONSOLE) $(LIBDIR_TESTNEWCONSOLE) $(OBJ_TESTNEWCONSOLE) $(LIB_TESTNEWCONSOLE) -o $(OUT_TESTNEWCONSOLE)

